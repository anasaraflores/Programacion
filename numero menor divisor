
#include "stdafx.h"
#include <iostream>
#include "conio.h"
 using namespace std;

 void main()
 { int a,b, mayor, menor;
    cout <<"Ingrese el primer numero: "<< endl;
    cin >> a;
	cout <<"Ingrese el segundo numero: "<< endl;
	cin >> b;
	 if(a<b)
	  {mayor=b;
	   menor=a;
	  }	 
	 else
	 { mayor=a;
	   menor=b;
	 }
	 if(mayor%menor==0)
	 {cout <<" es divisor:" << endl;
	 }
	 else
	 {cout <<" no es divisor:"<<endl;
	 }
getch();
 }

